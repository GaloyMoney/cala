{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "TransactionEvent",
  "oneOf": [
    {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "const": "initialized"
        },
        "values": {
          "$ref": "#/$defs/TransactionValues"
        }
      },
      "required": [
        "type",
        "values"
      ]
    },
    {
      "type": "object",
      "properties": {
        "fields": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "type": {
          "type": "string",
          "const": "updated"
        },
        "values": {
          "$ref": "#/$defs/TransactionValues"
        }
      },
      "required": [
        "type",
        "values",
        "fields"
      ]
    }
  ],
  "$defs": {
    "TransactionValues": {
      "type": "object",
      "properties": {
        "correlation_id": {
          "type": "string"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "effective": {
          "type": "string",
          "format": "date"
        },
        "entry_ids": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "uuid"
          }
        },
        "external_id": {
          "type": [
            "string",
            "null"
          ]
        },
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "journal_id": {
          "type": "string",
          "format": "uuid"
        },
        "metadata": true,
        "modified_at": {
          "type": "string",
          "format": "date-time"
        },
        "tx_template_id": {
          "type": "string",
          "format": "uuid"
        },
        "version": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0
        },
        "void_of": {
          "type": [
            "string",
            "null"
          ],
          "format": "uuid"
        },
        "voided_by": {
          "type": [
            "string",
            "null"
          ],
          "format": "uuid"
        }
      },
      "required": [
        "id",
        "version",
        "created_at",
        "modified_at",
        "journal_id",
        "tx_template_id",
        "entry_ids",
        "effective",
        "correlation_id"
      ]
    }
  }
}
